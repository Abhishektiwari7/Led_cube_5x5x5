void ClearOut(){
  digitalWrite(latchPin, LOW);
  for(int b=0; b<6; b++){
    SPI.transfer(B00000);
  }
  digitalWrite(latchPin, HIGH);
  digitalWrite(latchPin, LOW);
}

void shift_out(const byte  animation[5][6], int animationTime){
  for(int i=0; i<(animationTime*0.2);i++){ 
    for(int a=0; a<5; a++){
      digitalWrite(latchPin, LOW);
      for(int b=0; b<6; b++){
        SPI.transfer(animation[a][b]);
      }
      digitalWrite(latchPin, HIGH);
      delay(1);
    }
  }
}


